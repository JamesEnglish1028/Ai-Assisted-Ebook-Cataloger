{
  "name": "ai-assisted-ebook-cataloger",
  "version": "1.0.0",
  "description": "AI-powered ebook cataloging tool with REST API for library classification, metadata extraction, and intelligent summaries using Google Gemini",
  "type": "module",
  "author": "James English",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently \"npm run server\" \"npm run dev\"",
    "dev:all-with-bridge": "concurrently \"npm run server\" \"npm run loc-mcp-bridge\" \"npm run dev\"",
    "dev:all-with-bridges": "concurrently \"npm run server\" \"npm run loc-mcp-bridge\" \"npm run open-library-mcp-bridge\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node --import tsx/esm server/index.ts",
    "loc-mcp-bridge": "node --import tsx/esm server/locAuthorityBridge.ts",
    "open-library-mcp-bridge": "node --import tsx/esm server/openLibraryBridge.ts",
    "server:watch": "nodemon --exec 'node --import tsx/esm' server/index.ts",
    "server:prod": "tsx server/index.ts",
    "start": "NODE_ENV=production node --import tsx/esm server/index.ts",
    "render-build": "npm ci && npm run build && npm run postbuild",
    "postbuild": "echo 'Build completed successfully'",
    "ci:render-api": "npm run test -- --runInBand tests/textAnalysis.test.ts tests/openLibraryEnrichment.test.ts tests/aiProviderOpenLibraryPrompt.test.ts tests/hardcoverEnrichment.test.ts tests/aiProviderHardcoverPrompt.test.ts",
    "ci:render-web": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@google/genai": "^1.24.0",
    "@types/pdf-parse": "^1.1.5",
    "@xmldom/xmldom": "^0.8.11",
    "canvas": "^3.2.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "jszip": "^3.10.1",
    "multer": "^2.0.2",
    "pdf-parse": "^2.3.0",
    "pdfjs-dist": "^5.4.296",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.10",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
