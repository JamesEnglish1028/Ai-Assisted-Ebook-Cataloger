{
  "name": "ai-assisted-ebook-cataloger",
  "version": "1.0.0",
  "description": "AI-powered ebook cataloging tool with REST API for library classification, metadata extraction, and intelligent summaries using Google Gemini",
  "type": "module",
  "author": "James English",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node --import tsx/esm server/index.ts",
    "server:watch": "nodemon --exec 'node --import tsx/esm' server/index.ts",
    "server:prod": "tsx server/index.ts",
    "start": "NODE_ENV=production node --import tsx/esm server/index.ts",
    "render-build": "npm ci && npm run build && npm run postbuild",
    "postbuild": "echo 'Build completed successfully'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@daisy/ace": "^1.3.7",
    "@daisy/ace-axe-runner-puppeteer": "^1.3.7",
    "@google/genai": "^1.24.0",
    "@types/node-fetch": "^2.6.13",
    "@types/pdf-parse": "^1.1.5",
    "@xmldom/xmldom": "^0.8.11",
    "canvas": "^3.2.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "jszip": "^3.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^2.3.0",
    "pdfjs-dist": "^5.4.296",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsx": "^4.20.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.10",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
